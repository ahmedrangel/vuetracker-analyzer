{
  "cloudflare": {
    "metas": {
      "slug": "cloudflare",
      "name": "Cloudflare",
      "icon": "cloudflare",
      "url": "https://cloudflare.com"
    },
    "detectors": {
      "headers": {
        "server": "cloudflare",
        "cf-ray": true,
        "cf-cache-status": true
      }
    }
  },
  "vercel": {
    "metas": {
      "slug": "vercel",
      "name": "Vercel",
      "icon": "vercel",
      "url": "https://vercel.com"
    },
    "detectors": {
      "headers": {
        "server": "vercel",
        "x-vercel-id": true,
        "x-vercel-cache": true
      }
    }
  },
  "netlify": {
    "metas": {
      "slug": "netlify",
      "name": "Netlify",
      "icon": "netlify",
      "url": "https://www.netlify.com"
    },
    "detectors": {
      "headers": {
        "server": "netlify",
        "x-nf-request-id": true
      }
    }
  },
  "deno": {
    "metas": {
      "slug": "deno",
      "name": "Deno",
      "icon": "deno",
      "url": "https://deno.com"
    },
    "detectors": {
      "headers": {
        "server": ["deployd", "deno*"],
        "via": "*prod.deno-cluster.net",
        "x-deno-trace-id": true
      }
    }
  },
  "apache": {
    "metas": {
      "slug": "apache",
      "name": "Apache",
      "icon": "apache",
      "url": "https://apache.org"
    },
    "detectors": {
      "headers": {
        "server": "apache"
      }
    }
  },
  "cloudfront": {
    "metas": {
      "slug": "cloudfront",
      "name": "CloudFront",
      "icon": "aws-cloudfront",
      "url": "https://aws.amazon.com/cloudfront/"
    },
    "detectors": {
      "headers": {
        "server": "cloudfront",
        "via": "*cloudfront*",
        "x-amz-cf-id": true,
        "x-amz-cf-pop": true
      }
    }
  },
  "akamai": {
    "metas": {
      "slug": "akamai",
      "name": "Akamai",
      "icon": "akamai",
      "url": "https://www.akamai.com"
    },
    "detectors": {
      "headers": {
        "akamai-grn": true,
        "x-akam-sw-version": true,
        "x-akamai-transformed": true,
        "x-akamai-request-id": true
      }
    }
  },
  "nginx": {
    "metas": {
      "slug": "nginx",
      "name": "Nginx",
      "icon": "nginx",
      "url": "https://nginx.org"
    },
    "detectors": {
      "headers": {
        "server": "nginx*"
      }
    }
  },
  "s3": {
    "metas": {
      "slug": "s3",
      "name": "Amazon S3",
      "icon": "aws-s3",
      "url": "https://aws.amazon.com/s3/"
    },
    "detectors": {
      "headers": {
        "server": "AmazonS3"
      }
    }
  },
  "heroku": {
    "metas": {
      "slug": "heroku",
      "name": "Heroku",
      "icon": "heroku",
      "url": "https://www.heroku.com/"
    },
    "detectors": {
      "headers": {
        "server": "heroku",
        "via": "*heroku-router"
      }
    }
  }
}
